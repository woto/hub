<p id="notice"><%= notice %></p>

<h1>Template2s</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Gender</th>
      <th>Foo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @template2s.each do |template2| %>
      <tr>
        <td><%= template2.name %></td>
        <td><%= template2.gender %></td>
        <td><%= template2.foo %></td>
        <td><%= link_to 'Show', template2 %></td>
        <td><%= link_to 'Edit', edit_template2_path(template2) %></td>
        <td><%= link_to 'Destroy', template2, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Template2', new_template2_path %>
